mediaGCCSemO3 = []
stdGCCSemO3 = []
mediaGCCComO3 = []
stdGCCComO3 = []

for i in range(5):
	mediaGCCSemO3.append(np.mean(gccSemO3[i]))
	stdGCCSemO3.append(np.std(gccSemO3[i]))
	mediaGCCComO3.append(np.mean(gccComO3[i]))
	stdGCCComO3.append(np.std(gccComO3[i]))

fig = plt.figure(figsize=(10,5))
ax1 = fig.add_subplot(111)


## MÃ©dia  ##

# plt.plot(np.array(range(len(mediaSemO3)))*2, mediaSemO3, label=r'Sem -O3', color='#1F77B4', marker='o', lw=2, markersize=8, markeredgecolor='#1F77B4', markerfacecolor='#FFFFFF')
# plt.plot(np.array(range(len(mediaComO3)))*2, mediaComO3, label=r'Com -O3', color='#D62728', marker='s', lw=2, markersize=8, markeredgecolor='#D62728', markerfacecolor='#FFFFFF')


## MÃ©dia e desvio padrÃ£o ##

plt.errorbar(np.array(range(len(mediaGCCSemO3)))*2, mediaGCCSemO3, stdGCCSemO3,label=r'gcc sem -O3', color='#2CA02C', fmt='v', lw=1, capsize=4, markersize=8, markeredgecolor='#2CA02C', markerfacecolor='#95CF95')
plt.plot(np.array(range(len(mediaGCCSemO3)))*2, mediaGCCSemO3, color='#2CA02C',lw=2)

plt.errorbar(np.array(range(len(mediaGCCComO3)))*2, mediaGCCComO3, stdGCCComO3,label=r'gcc com -O3', color='#FF7F0E', fmt='D', lw=1, capsize=4, markersize=8, markeredgecolor='#FF7F0E', markerfacecolor='#FFBF86')
plt.plot(np.array(range(len(mediaGCCComO3)))*2, mediaGCCComO3, color='#FF7F0E',lw=2)



ax1.yaxis.grid(True, linestyle='-', linewidth=0.5, which='major', color='lightgrey',alpha=1.0)
# ax1.xaxis.grid(True, linestyle='-', linewidth=0.5, which='major', color='lightgrey',alpha=1.0)

fontSize=12

ax1.set_axisbelow(True)
# ax1.set_title('TÃ­tulo', fontsize=fontSize+3, fontweight='bold')
ax1.set_xlabel(r'Tamanho matriz', fontsize=fontSize+2)
ax1.set_ylabel('Tempo medio (em seg)', fontsize=fontSize+2)
ax1.xaxis.set_label_coords(0.5, -0.08)
ax1.set_facecolor('white')


handles, labels = ax1.get_legend_handles_labels()
leg = ax1.legend(handles, labels, bbox_to_anchor=(0.01, 0.98), loc=2, borderaxespad=0., fontsize=fontSize)
leg.get_frame().set_facecolor('#FFFFFF')
plt.legend(title=r'Compilador')




ticks = ['100', '200', '300', '500', '700']
plt.xticks(range(0, len(ticks)*2, 2), ticks, fontsize=fontSize)
plt.yticks(fontsize=fontSize)
plt.xlim(-1, len(ticks)*2-1)
#plt.ylim(-10, 40)


plt.tight_layout()
plt.show()

